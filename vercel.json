{
  "crons": [
    {
      "path": "/api/cron/check",
      "schedule": "0 14 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/api-docs",
      "destination": "/api-docs/index.html",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/api/auth/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ]
}
